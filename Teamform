# Complete the function below.

def teamFormation(score, team, m):
    x = score
    res = 0
    for i in range(0,team):
        y = x[:m]+x[-m:]
        mx = max(y)
        z = x.index(mx)
        res = res+mx
        if z < m:
            del x[z]
        else:
            z = x[::-1].index(mx)
            del x[-(z+1)]
            
        
        #print('X: ' + str(x))
        #print('z: ' + str(z))
    return res;
